    "@types/mocha": "^9.1.1",
    "@types/nodemailer": "^6.4.4",
    "@types/sinon": "^10.0.12",
    "@types/sinon-chai": "^3.2.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.20.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
