    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.7",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "type-graphql-dataloader": "^0.5.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.45",
    "typeorm-typedi-extensions": "^0.4.1",
    "typescript": "^4.7.4",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
